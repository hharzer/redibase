{
  "name": "redibase",
  "version": "1.0.39",
  "description": "redibase the redis powered firebase alternative",
  "main": "build/index.js",
  "scripts": {
    "test": "node --inspect-brk=9876 ./node_modules/jest/bin/jest.js",
    "build": "tsc",
    "deploy": "npm version patch && npm run build",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "lint": "tsc --noEmit && eslint *.{js,ts,tsx} --quiet --fix",
    "q": "taskkill /f /im node.exe",
    "qm": "sudo killall -9 node"
  },
  "files": [
    "build/**/*"
  ],
  "jest": {
    "verbose": true,
    "maxConcurrency": 1,
    "testMatch": [
      "<rootDir>/tests/tests.test.ts"
    ],
    "testTimeout": 900000,
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "author": "",
  "contributors": [
    {
      "name": "Mendel Jacks",
      "email": "mendeljacks@gmail.com",
      "url": "http://mendeljacks.com"
    },
    {
      "name": "Daniel Jacks",
      "email": "danielchaimjacks@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mendeljacks/redibase.git"
  },
  "license": "MIT",
  "keywords": [
    "redis",
    "firebase",
    "realtime",
    "database",
    "alternative"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "ioredis": "^4.16.1",
    "ramda": "^0.27.0",
    "redis-evalsha": "^1.1.1",
    "serialize-javascript": "^3.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "blns": "^2.0.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.2.3",
    "sucrase": "^3.13.0",
    "typescript": "^3.8.3"
  }
}
